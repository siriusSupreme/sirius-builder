{
  "name": "siriussupreme/sirius-builder",
  "description": "基于 AdminLTE 的表单构建器",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/siriusSupreme/sirius-builder",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "siriusSupreme",
      "email": "guoplc@163.com",
      "homepage": "https://github.com/siriusSupreme/sirius-builder",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "almasaeed2010/adminlte": "~2.4",
    "siriussupreme/sirius-filesystem": "^1.0",
    "siriussupreme/sirius-support": "^1.0",
    "symfony/dom-crawler": "~3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "symfony/css-selector": "~3.1",
    "fzaninotto/faker": "~1.4",
    "intervention/image": "~2.3",
    "topthink/framework": "^5.1.2",
    "laravel/browser-kit-testing": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sirius\\Builder\\": "src/"
    },
    "classmap": [],
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Models\\": "tests/models",
      "Tests\\Controllers\\": "tests/controllers"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "suggest": {
    "intervention/image": "Required to handling and manipulation upload images (~2.3).",
    "spatie/eloquent-sortable": "Required to built orderable gird."
  },
  "extra": {},
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "pre-install-cmd": [],
    "post-install-cmd": [],
    "pre-update-cmd": [],
    "post-update-cmd": [],
    "pre-status-cmd": [],
    "post-status-cmd": [],
    "pre-package-install": [],
    "post-package-install": [],
    "pre-package-update": [],
    "post-package-update": [],
    "pre-package-uninstall": [],
    "post-package-uninstall": [],
    "pre-autoload-dump": [],
    "post-autoload-dump": [],
    "post-root-package-install": [],
    "post-create-project-cmd": []
  },
  "archive": {
    "exclude": []
  }
}
